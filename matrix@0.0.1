#include <iostream>
#include <sstream>
using namespace std;

bool matrix(int matrix[3][3])
{
	int element;
	bool result = true;
	for (int i = 0; i < 3; ++i) {
		for (int j = 0; j < 3; ++i) {
			cin >> element;
			istringstream stream(element);
			if (!(stream >> matrix[i][j])) {
				result = false;
				break;
			}
		}
		cout << endl;
	}
	return result;
}

int main()
{
	int matrix1[3][3];
	int matrix2[3][3];
	char znak;
	if ( matrix(matrix1) ) {
		cin >> znak;
		if ( matrix(matrix2 )) {
			if (znak == '+') {
				for (int i = 0; i < 3; i++) {
					for (int j = 0; j < 3; j++) {
						cout << (matrix1[i][j] + matrix2[i][j]) << " ";
					}
					cout << endl;
				}
			}
			else if (znak == '-') {
				for (int i = 0; i < 3; i++) {
					for (int j = 0; j < 3; j++) {
						cout << (matrix1[i][j] - matrix2[i][j]) << " ";
					}
					cout << endl;
				}
			}
			else if (znak == '*') {
				for (int i = 0; i < 3; i++) {
					for (int j = 0; j < 3; j++) {
						cout << (matrix1[i][j] * matrix2[i][j]) << " ";
					}
					cout << endl;
				}
			}
			else {
				cout << "An error has occured while reading sign";
				return -1;
			}
		}
		else {
			cout << "An error has occured while reading numbers from line";
			return -1;
		}
	}
	else {
		cout << "An error has occured while reading numbers from line";
	}
}

