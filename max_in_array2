#include <iostream>
#include <sstream>
using namespace std;

int main() {
    int numbers1[10], numbers2[10], max1 = 0, max2 = 0;
    bool failure = false;
    for (string string; getline(cin, string);) {
        istringstream stream(string);
        for (int i = 0; i < 10; ++i) {
            if (!(stream >> numbers1[i])) {
                failure = true;
                break;
            }
        }
        if (!failure) {
            max1 = numbers1[0];
            for (int i = 1; i < 10; i++) {
                if (numbers1[i] > max1) max1 = numbers1[i];
            }
            break;
        } else {
            std::cout << "An error has occured while reading numbers from line" << endl;
            break;
        }
    }
    if (!failure) {
        for (string string; getline(cin, string);) {

            istringstream stream(string);
            bool failure2 = false;
            for (int i = 0; i < 10; ++i) {
                if (!(stream >> numbers2[i])) {
                    failure2 = true;
                    break;
                }
            }
            if (!failure2) {
                max2 = numbers2[0];
                for (int i = 1; i < 10; i++) {
                    if (numbers2[i] > max2) max2 = numbers2[i];
                }
                break;
            } else {
                cout << "An error has occured while reading numbers from line" << endl;
                break;
            }
        }
    }
    if (!failure) {cout<<max1+max2;}
}
