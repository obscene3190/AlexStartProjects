#include <iostream>
#include <sstream>
using namespace std;

int main()
{   
    int numbers[ 10 ];
    for( string string; getline(cin, string); ) {
        
        istringstream stream( string );
        bool failure = false;
        for( int i = 0; i < 10; ++i ) {
            if( !( stream >> numbers[ i ] ) ) {
                failure = true;
                break;
                }
            }
        if( !failure ) {
            int max = numbers[0];
            for(int i = 1; i< 10; i++) {
            if(numbers[i]>max) max=numbers[i]; }
            cout<<endl<<max;
            break;
            }
        else {
            std::cout << "An error has occured while reading numbers from line" << endl;
            break;
        }
    }
}
