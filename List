
#include <iostream>
#include <string>
using namespace std;

struct node_t {
  int value;
  node_t * next;
};

void init(node_t * list) {
    
}

void add(node_t * & head, int value) {
    node_t * node = new node_t;
    node->value = value;
    node->next = nullptr;
    if( head == nullptr) {
        head = node;
    }
    else {
        node_t * last = head;
        while(last->next) {
            last = last->next;
        }
        last->next = node;
    }
}

