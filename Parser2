#include <iostream>
#include <sstream>
#include <string>

using namespace std;

int main()
{
	string string;
	int itog, middle;
	char znak;
	getline(cin,string);
	istringstream stream(string);
	stream>>itog;
	while(stream>>znak) {
		stream>>middle;
		if(znak=='+') itog = itog+middle;
	        else if(znak=='-') itog = itog-middle;
	        else if(znak=='*') itog = itog*middle;
	        else if(znak=='/') {
			if (middle == 0) cout<<"Error";
			else itog = itog/middle;
			}
                else cout<<"Error";
	}
	cout<<itog;
}

