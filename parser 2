#include <iostream>

using namespace std;

int main() {
    float a, b, res;
    char znak;
    bool stop=false;
    cin >> a;
    cin >> znak;
    while (!stop) {
        if (znak == '+' || znak == '-' || znak == '/' || znak == '*') {
            cin >> b;
            if (znak == '+') {
                a = a + b;
            } else if (znak == '-') {
                a = a - b;
            } else if (znak == '*') {
                a = a * b;
            } else if (znak == '/')
                if (b == 0) {
                    cout << "Error occured";
                    return 0;
                } else a = a / b;
            cin >> znak;
        }
        else {
            stop = true;
            break;
            }
    }
    cout<<a;
}
