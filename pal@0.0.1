#include "stdafx.h"
#include <iostream>
#include <string>
using namespace std;

int palindrom(string name) {
	int res = 1, len = name.length();
	for (int i = 0; i < len/2; i++) {
		if (name[i] == name[len - 1 - i]) { res *= 1; }
		else { res *= 0; break; }
	}
	return res;
}
int main()
{
	string name;
	getline(cin, name);
	cout << palindrom(name);
	cin.get();
}
