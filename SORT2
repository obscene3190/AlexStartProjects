#include <iostream>
#include <utility>
#include <sstream>

using namespace std;

void selection_sort(double * array, unsigned int size) {
    unsigned int i, j;
    for (i = 1; i <  size; i++) {
        j = i;
        while (j > 0 and array[j] < array[j - 1]) {
             swap (array[j], array[j - 1]);
             j = j - 1;
        }
    }
}


bool read (istream & stream, double* &array, unsigned int &size) {
    bool res = true;
    for (unsigned int i=0; i < size; i++) {
        if (!(stream >> array[i])) {
            res = false;
            break;
        }
    }
    return res;
}

void write(ostream & stream,double* array, unsigned int size) {
    for (unsigned int i = 0; i < size; i++) {
        stream << array[i] << ' ';
    }
}

int main() {
    unsigned int size;
    double* array;
    if(!(cin >> size)) {
        cout << "An error has occured while reading numbers from line";
        return 1;
    }
    cin.get();
    string string;
    getline(cin, string);
    istringstream stream(string);
    array = new double [size];
    if( read(stream,array, size)) {
        selection_sort (array, size);
        write (cout, array, size);
    }
    else {
        cout << "An error has occured while reading numbers from line";
    }
    delete[] array;
